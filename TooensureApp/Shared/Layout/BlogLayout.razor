@inject NavigationManager navMan;


<section class="bg-white dark:bg-slate-700 px-6 py-8 ring-1 ring-slate-500/5 shadow-xl">
      <section >
        <div>
          <span class="inline-flex mt-5 items-center justify-center p-2 rounded-md shadow-lg">
            <a @onclick="OnBlogClicked">
              <img src="img/readyvsgoogle.png"/>
            </a>
          </span>
        </div>
        
        <!--  -->
        <h3 @onclick="OnBlogClicked" class="text-slate-800 dark:text-white mt-5 text-base font-medium tracking-tight">Apps | Ready</h3>
        <h2 @onclick="OnBlogClicked" class="text-slate-700 dark:text-white mt-1 font-bold  tracking-tight text-3xl">Can’t log into Ready,  Unknown Error</h2>
        <p @onclick="OnBlogClicked" class="text-slate-500 dark:text-slate-400 mt-5 text-lg">
          App is in review and will be updated with new anomation and login system....
        </p>
        <div @onclick="OnBlogClicked" class="text-right dark:text-slate-300 underline mt-5 text-lg">Let's Go</div>
      
      </section>
      <section class="my-10">
        <h2 class="text-slate-800 dark:text-white mt-1 font-bold underline  tracking-tight text-3xl">Latest</h2>
      </section>
      <section @onclick="OnBlogClicked" class="border-b-2 ">
        <p class="text-slate-800 dark:text-white mt-5 text-base font-medium tracking-tight">Releases | Ready</p>
        <h5 class="text-slate-800 dark:text-white mt-1 font-bold  tracking-tight text-1xl">Can’t log into Ready,  Unknown Error</h5>
        <p class="text-slate-500 mb-4 dark:text-slate-400 mt-5 text-sm">
          Intergrated google authentication so users can easyily and seculy access Ready App.        </p>
      </section>
    </section>

@code
{
    private void OnBlogClicked()
    {
        navMan.NavigateTo("/blog");
    }
}